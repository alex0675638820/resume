<div>
    {{#with heading}}
        <div class="card">
            <div class="card-body">
                <div class="card">
                    <div class="card-body">
                        {{#with main}}
                            <h1>{{title}}</h1>
                            <span class="text-muted">{{text}}</span>
                            <br>
                            <br>
                            {{#with button_text}}
                                <button class="btn btn-primary">{{solid}}</button>
                                <br>
                                <br>
                                <button class="btn btn-outline-primary">{{outline}}</button>
                            {{/with}}
                           <br>
                        {{/with}}
                    </div>
                </div>
                <br>
                <div class="card">
                    <div class="card-body">
                        {{#with second}}
                            <h2>{{title}}</h2>
                            <span class="text-muted">{{text}}</span>
                            <br>
                            <br>
                            {{#with button_text}}
                                <button class="btn btn-secondary">{{solid}}</button>
                                <br>
                                <br>
                                <button class="btn btn-outline-secondary">{{outline}}</button>
                            {{/with}}
                           <br>
                        {{/with}}
                    </div>
                </div>
                <br>
                <div class="card">
                    <div class="card-body">
                        {{#with subblock}}
                            <h3>{{title}}</h3>
                            <span class="text-muted">{{text}}</span>
                            <br>
                            <br>
                            {{#with button_text}}
                                <button class="btn btn-dark">{{solid}}</button>
                                <br>
                                <br>
                                <button class="btn btn-outline-dark">{{outline}}</button>
                            {{/with}}
                           <br>
                        {{/with}}
                    </div>
                </div>
            </div>
        </div>
    {{/with}}
    <br>
    <br>
    {{#with nested_display}}
        <div class="card">
            <div class="card-body">
               <div class="display-1">{{title}}</div> 
               <br>
               <div class="card">
                    <div class="card-body">
                        {{#with children_display}}
                            <div class="display-4">{{title}}</div>
                            <br>
                            <span class="text-muted">{{text}}</span>
                            <br>
                            <br>
                            {{#with button}}
                                <button class="btn btn-outline-warning">{{text}}</button>
                            {{/with}}
                        {{/with}}
                    </div>
                </div>
                <br>
                <h6>{{header_text}}</h6>
                <br>
                {{#with button}}
                    <button class="btn btn-info">{{text}}</button>
                {{/with}}
            </div>
        </div>
    {{/with}}
    <br>
    <br>
    {{#with paragraph}}
        <div class="card">
            <div class="card-body">
               <span class="text-muted text-uppercase">{{title}}</span>
               <br>
               <span class="text-muted text-lowercase">{{text}}</span> 
               <br>
               {{#with button}}
                    <br>
                    <a href='url' class=" btn-primary">{{text}}</a>
               {{/with}}
               <br>
               <br>
               <div class="card">
                    <div class="card-body">
                        <ul>
                            {{#each list_name}}
                                <li><a href='url' class=" btn-primary">{{text}}</a></li>
                                <br>
                            {{/each}}
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    {{/with}}
</div>